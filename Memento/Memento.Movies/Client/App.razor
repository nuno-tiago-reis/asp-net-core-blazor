<Router AppAssembly="@typeof(Program).Assembly">
	<Found Context="routeData">
		<AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(AppLayout)">
			<Authorizing>
				Authorizing...
			</Authorizing>
			<NotAuthorized>
				Not Authorized...
			</NotAuthorized>
		</AuthorizeRouteView>
	</Found>
	<NotFound>
		<CascadingAuthenticationState>
			<LayoutView Layout="@typeof(AppLayout)">
				<NotFound></NotFound>
			</LayoutView>
		</CascadingAuthenticationState>
	</NotFound>
</Router>